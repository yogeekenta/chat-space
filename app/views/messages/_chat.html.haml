.header
  .left-header
    %h2.left-header__title
      =@group.name
    %ul.left-header__members
      %li.member Member:
      %li.member= current_user.name
      - @group.users.each do |user|
        -if current_user.id != user.id  
          %li.member= user.name
  .right-header
    .right-header__button
      = link_to edit_group_path(@group) do
        Edit